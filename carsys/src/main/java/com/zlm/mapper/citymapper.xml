<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zlm.dao.CityDao">
<select id="selectByPid" parameterType="int" resultType="city">
    select * from sys_city where pid = #{pid}
</select>
    <select id="selectById" parameterType="string" resultType="string">
        select name from sys_city where id=#{id}
    </select>
</mapper>