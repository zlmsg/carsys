<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <typeAliases>
        <typeAlias type="com.zlm.entity.User" alias="user"></typeAlias>
        <typeAlias type="com.zlm.entity.Car" alias="car"></typeAlias>
        <typeAlias type="com.zlm.entity.City" alias="city"></typeAlias>
        <typeAlias type="com.zlm.entity.Order" alias="order"></typeAlias>
    </typeAliases>

    <plugins>
        <!-- com.github.pagehelper为PageHelper类所在包名 -->
        <plugin interceptor="com.github.pagehelper.PageInterceptor">
            <!-- 使用下面的方式配置参数，后面会有所有的参数介绍 -->
            <property name="helperDialect" value="mysql"/>
        </plugin>
    </plugins>
</configuration>